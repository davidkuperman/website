<div class="admin_form">


<%= form_tag do %>
<fieldset>
<legend>Login</legend>
<div>
<label for="name" >Nombre:</label>
<%= text_field_tag :name, params[:name] %>
</div>
<div>
<label for="password" >Password:</label>
<%= password_field_tag :password, params[:password] %>
</div>
<div>
<%= submit_tag "Login" %>
</div>
</fieldset>
<% end %>

</div>